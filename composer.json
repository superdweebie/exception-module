{
    "name": "superdweebie/exception-module",
    "description": "Zend Framework 2 Module that provides exception handling functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "zf2"
    ],
    "homepage": "http://github.com/superdweebie/exceptionModule",
    "authors": [
        {
            "name": "Tim Roediger",
            "email": "superdweebie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/doctrine-mongo-odm-module" : "dev-master",
		"superdweebie/common"                : "~0.3",
        "zendframework/zend-http"            : "~2.1.5",
        "zendframework/zend-mvc"             : "~2.1.5",
        "zendframework/zend-service-manager" : "~2.1.5",
        "zendframework/zend-view"            : "~2.1.5"
    },
    "require-dev": {
        "zendframework/zend-test"    : "~2.1.5"
    },
    "suggest": {
        "superdweebie/sijit": "A dojo package that include a javascript client for this exception module"
    },
    "autoload": {
        "psr-0": {
            "Sds\\ExceptionModule": "src/"
        }
    }
}
{
    "name"       : "superdweebie/exception-module",
    "version"    : "0.3",
    "description": "Zend Framework 2 Module that provides exception handling functionality",
    "type"       : "library",
    "license"    : "MIT",
    "keywords": [
        "doctrine",
        "exception",
        "json",
        "zf2"
    ],
    "homepage"   : "http://github.com/superdweebie/exception-module",
    "authors"    : [
        {
            "name"  : "Tim Roediger",
            "email" : "superdweebie@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "superdweebie/doctrine-extensions-module" : "~0.3",
        "zendframework/zend-http"                 : "~2.2",
        "zendframework/zend-mvc"                  : "~2.2",
        "zendframework/zend-servicemanager"       : "~2.2",
        "zendframework/zend-view"                 : "~2.2"
    },
    "suggest": {
        "superdweebie/sijit": "A dojo package that includes a javascript client for this exception module"
    },
    "autoload": {
        "psr-0": {
            "Sds\\ExceptionModule": "src/"
        }
    }
}